
{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{png,jpg,gif,svg,js,css,woff,woff2}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
